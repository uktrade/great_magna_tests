[behave]
# PyCharm Behave runner does not support --junit parameter
# and this is causing some problems when running scenarios from within IDE
# https://youtrack.jetbrains.com/issue/PY-14219
# https://youtrack.jetbrains.com/issue/PY-18316
#junit=True
#junit_directory=./reports/
verbose=True
log_capture=False
logging_level=DEBUG

[behave.formatters]
mini = formatter.mini:Mini
